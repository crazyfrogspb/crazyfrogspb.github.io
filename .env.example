# Telegram API credentials
# Получить можно на https://my.telegram.org/apps
TELEGRAM_API_ID=your_api_id
TELEGRAM_API_HASH=your_api_hash

# OpenRouter API key для генерации саммари постов
# Получить можно на https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key

# HuggingFace API token для загрузки моделей
# Получить можно на https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=your_huggingface_token
